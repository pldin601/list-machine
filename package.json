{
  "name": "@peacefulbit/list-machine",
  "version": "0.1.6",
  "description": "Lisp-like interpreter written on Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "ts-node src/repl.ts",
    "start": "node dist/repl.js",
    "test": "jest",
    "lint": "tslint src/",
    "cover": "jest --coverage && (cat ./coverage/lcov.info | coveralls)",
    "build": "npm run clean && tsc",
    "prepublish": "npm run build"
  },
  "keywords": [],
  "author": "Roman Lakhtadyr <roman.lakhtadyr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/lodash": "^4.14.80",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/webpack": "^3.0.14",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "npm": "^5.5.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.1.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/winston": "^2.3.7",
    "@types/xterm": "^2.0.3",
    "compose-function": "^3.0.3",
    "is-primitive": "^2.0.0",
    "lodash": "^4.17.4",
    "winston": "^2.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "roots": [
      "src"
    ]
  }
}
