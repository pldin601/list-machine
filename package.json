{
  "name": "list-machine",
  "version": "0.1.0",
  "description": "Lisp-like interpreter written on Node.js",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/repl.ts",
    "start": "node dist/repl.js",
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [],
  "author": "Roman Lakhtadyr <roman.lakhtadyr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/lodash": "^4.14.77",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/winston": "^2.3.6",
    "is-primitive": "^2.0.0",
    "lodash": "^4.17.4",
    "winston": "^2.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "roots": [
      "src"
    ]
  }
}
